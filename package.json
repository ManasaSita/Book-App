{
    "name": "bookshelf-app",
    "version": "1.0.0",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm start",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "devDependencies": {
        "concurrently": "^6.2.0"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "react-icons": "^5.3.0",
        "react-router-dom": "^6.25.0"
    }
}
